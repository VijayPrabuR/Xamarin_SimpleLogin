<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RegLoginDemo.Views.HomePage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand">
            <Label Text="Welcome " FontSize="25"/>
            <Label x:Name="usr" FontSize="25" FontAttributes="Bold"/>
            <Button Text="Logout" HorizontalOptions="Center" BackgroundColor="Blue" CornerRadius="30" Clicked="LogoutButton_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
    